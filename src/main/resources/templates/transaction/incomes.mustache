{{>layout/header}}

<div class="container px-5">
    <h2>Incomes</h2>
    <table class="table table-striped-columns table-bordered table-hover">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Amount</th>
            <th scope="col">Date</th>
            <th scope="col">Source</th>
            <th scope="col">Account</th>
        </tr>
        </thead>
        <tbody>
        {{#incomeList}}
            <tr>
                <th scope="row">{{id}}</th>
                <td>{{amount}}</td>
                <td>{{date}}</td>
                <td>{{source}}</td>
                <td>{{accounts.name}}</td>
            </tr>
        {{/incomeList}}
        </tbody>
    </table>
</div>



{{>layout/footer}}
